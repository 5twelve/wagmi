{
  "name": "@wagmi/devtools",
  "description": "Devtools for Wagmi",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wevm/wagmi.git",
    "directory": "packages/devtools"
  },
  "scripts": {
    "build": "pnpm clean && pnpm build:esm+types",
    "build:esm+types": "tsc --project tsconfig.build.json --outDir ./dist/esm --declaration --declarationMap --declarationDir ./dist/types",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo react solid",
    "test:build": "publint --strict && attw --pack --ignore-rules cjs-resolves-to-esm"
  },
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.test-d.ts",
    "react/**",
    "solid/**"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/esm/exports/index.js",
  "types": "./dist/types/exports/index.d.ts",
  "typings": "./dist/types/exports/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/exports/index.d.ts",
      "default": "./dist/esm/exports/index.js"
    },
    "./react": {
      "types": "./dist/types/exports/react.d.ts",
      "default": "./dist/esm/exports/react.js"
    },
    "./solid": {
      "types": "./dist/types/exports/solid.d.ts",
      "default": "./dist/esm/exports/solid.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "react": ["./dist/types/exports/react.d.ts"],
      "solid": ["./dist/types/exports/solid.d.ts"]
    }
  },
  "peerDependencies": {
    "@tanstack/react-query": ">=5.0.0",
    "@tanstack/solid-query": ">=5.59.0",
    "@wagmi/solid": "workspace:*",
    "react": ">=18",
    "solid-js": ">=1.8.20",
    "typescript": ">=5.0.4",
    "viem": "2.x",
    "wagmi": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": true
    },
    "@tanstack/solid-query": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "solid-js": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "wagmi": {
      "optional": true
    }
  },
  "dependencies": {
    "@wagmi/devtools-ui": "workspace:*"
  },
  "devDependencies": {
    "@tanstack/react-query": "catalog:",
    "@tanstack/solid-query": "catalog:",
    "@types/react": "catalog:",
    "@wagmi/solid": "workspace:*",
    "react": "catalog:",
    "solid-js": "catalog:",
    "wagmi": "workspace:*"
  },
  "contributors": ["awkweb.eth <t@wevm.dev>", "jxom.eth <j@wevm.dev>"],
  "funding": "https://github.com/sponsors/wevm",
  "keywords": [
    "wagmi",
    "react",
    "hooks",
    "eth",
    "ethereum",
    "dapps",
    "wallet",
    "web3"
  ]
}
